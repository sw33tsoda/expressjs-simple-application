<table class="table">
    <thead class="thead-dark">
      <tr>
        <th>Id</th>
        <th>Nickname</th>
        <th>Description</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody>

      <% data.forEach(user => { %>
        <tr>
          <td><%= user.id %></td>
          <td><%= user.nickname %></td>
          <td class="small"><%= user.description %></td>
          <td>
            <a href="/users/edit/<%= user.id %>" class="btn btn-warning">Edit</a>
            <a href="/users/delete/<%= user.id %>" class="btn btn-danger">Delete</a>
          </td>
        </tr>
      <% }); %>
      
    </tbody>
  </table>
