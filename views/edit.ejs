<div class="jumbotron send-form">
    <h1>Edit message</h1>
    <p>Edit something...</p>
    <form method="POST" action="/users/edit/update">
        <input type="hidden" value="<%= data.id; %>" name="id">
        <label for="nickname">Nickname</label>
        <input type="text" name="nickname" class="form-control" placeholder="Insert your nickname" value="<%= data.nickname; %>">
        <br>
        <label for="description">Description</label>
        <input type="text" name="description" class="form-control" placeholder="What do you want to say?" value="<%= data.description; %>">
        <br>
        <input type="submit" class="btn btn-danger">
    </form>
</div>